<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="seq_job_requisition" model="ir.sequence">
        <field name="name">Job requisition sequence</field>
        <field name="code">job.requisition</field>
        <field name="prefix">JR</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="seq_job_description" model="ir.sequence">
        <field name="name">Job description sequence</field>
        <field name="code">job.description</field>
        <field name="prefix">JD</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>
</odoo>
