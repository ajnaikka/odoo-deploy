<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sm_enquiry_warning_form" model="ir.ui.view">
            <field name="name">sm_enquiry_warning_form</field>
            <field name="model">enquiry.warning</field>
            <field name="arch" type="xml">
                <form>
                    <group col="4" colspan="4">
                        <field name="message" readonly="1" nolabel="1"/>
                    </group>
                    <footer>
                        <button string="Update" name="update_partner_record" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_enquiry_warning" model="ir.actions.act_window">
            <field name="name">Warning Enquiry</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">enquiry.warning</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
    </data>
</odoo>