    <template id="uaa_main_website_layout" name="Main Uaa Website Layout">
        <html lang="en" async="">
            <head>
                <meta charset="utf-8"/>
                <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
                <meta http-equiv="x-ua-compatible" content="ie=edge"/>
                <title>Universal Airport Assistance</title>
                <meta name="robots" content="noindex, follow"/>
                <meta name="description" content=""/>
                <!-- Favicon -->
                <link rel="icon" href="/sr_uaa_main/static/src/faveicon.png"/>

                <t t-call-assets="sr_uaa_main.website_assets" t-js="false"/>
            </head>
            <body style="font-family:Arial,Helvetica,sans-serif !important;">
                <t t-call="sr_uaa_main.uaa_website_header"/>
                <main class="mt-30">
                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                    <t t-raw="0"/>
                </main>
                <a id="scrollUp" href="#top" style="position: fixed; z-index: 2147483647;">
                    <i class="lastudioicon-up-arrow"></i>
                </a>

                <t t-call-assets="web.assets_common_minimal_js" t-css="false" defer_load="True"/>
                <t t-call-assets="web.assets_frontend_minimal_js" t-css="false" defer_load="True"/>
                <t t-call="web.conditional_assets_tests"/>
                <t t-call-assets="web.assets_common_lazy" t-css="false" lazy_load="True"/>
                <t t-call-assets="web.assets_frontend_lazy" t-css="false" lazy_load="True"/>

                <t t-call="sr_uaa_main.uaa_website_footer"/>
                <t t-call-assets="sr_uaa_main.website_assets" t-css="false"/>
            </body>
        </html>
    </template>

