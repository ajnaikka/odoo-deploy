            <!--                            <p t-if="success" class="form_submit_success">Booked Successfully</p>-->
            <t t-else="">
                <form action="/request-meet-greet"
                      id="meet_greet_request_form" method="post" accept-charset="utf-8"
                      style="margin-top: 50px;"
                >
                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                    <input type="hidden" id="timezone" name="timezone"></input>
                    <div id="div_meet_first_page"
                         t-attf-class="uaa_form_div #{second_page and  'tab_hide' or ''}">
                        <h1>Airport and Personal Details</h1>

                        <div class="form-group">
                            <label>Airport
                                <span class="required-field"></span>
                            </label>
                            <style>

                                span.select2.select2-container.select2-container--default,
                                span.select2.select2-container.select2-container--default,
                                .select2-container--open .select2-dropdown--above { max-width: 100%
                                !important;min-width:160px;}
                                .form-group.contact_number { overflow: hidden;}
                                header .navbar-default .navbar-nav>li>a.active { color: #0eb4df
                                !important;}
                                @media screen and (max-width 768px){
                                /*dons*/
                                nav.navbar ul.dropdown-menu.show{position: static; float: none; width:
                                auto; margin-top: 0; background-color: transparent; border: 0;
                                box-shadow: none;}
                                .navbar-default .navbar-nav .dropdown-menu>li>a { color: #777;}
                                }


                            </style>
                            <select name="airport_name" id="airport_id"
                                    t-attf-class="form-control #{error.get('airport_name') and 'is-invalid' or ''}">
                                <option value="">Select Airport</option>
                                <t t-foreach="airports" t-as="airport">
                                    <option t-att-value="airport.id"
                                            t-att-selected="airport.id == airport_name">
                                        <span t-esc="airport.name"/>
                                        <span>(</span>
                                        <span t-esc="airport.code"/>
                                        <span>)</span>
                                        <t t-if="airport.country_id and airport.country_id.name">
                                            <span>/</span>
                                            <span t-esc="airport.country_id.name"/>
                                        </t>
                                    </option>
                                </t>
                            </select>
                            <span t-if="error.get('airport_name')" t-esc="error.get('airport_name')"
                                  id="err_origin_code" class="err_msg"/>
                        </div>
                        <div class="form-group">
                            <label>Name of the Traveller
                                <span class="required-field"></span>
                            </label>
                            <input type="text"
                                   t-attf-class="form-control #{error.get('traveler_name') and 'is-invalid' or ''}"
                                   name="traveler_name" id="traveler_name" t-att-value="traveler_name"/>
                            <span t-if="error.get('traveler_name')" t-esc="error.get('traveler_name')"
                                  id="err_name" class="err_msg"/>
                        </div>
                        <div class="form-group">
                            <label>Email
                                <span class="required-field"></span>
                            </label>
                            <input type="text"
                                   t-attf-class="form-control #{error.get('email') and 'is-invalid' or ''}"
                                   name="email" t-att-value="email"/>
                            <span t-if="error.get('email')" t-esc="error.get('email')"
                                  id="err_email" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <div class="form-group contact_number">
                            <label>Contact Number
                                <span class="required-field"></span>
                            </label>
                            <div class="input-group contact_number">

                                <select t-attf-class="form-control input-group-addon #{error.get('country_code') and 'is-invalid' or ''}"
                                        name="country_code"
                                        id="country_code">
                                    <option value="">Select Country Code</option>

                                    <t t-foreach="countries" t-as="country">
                                        <option t-att-value="country.id"
                                                t-att-selected="country_code == country.id">
                                            <!--                                                            <div>-->
                                            <!--                                                                <img height="30px" width="30px" t-att-src="country.image_url"/>-->
                                            <span>
                                                <!--                                                                    <span t-esc="country.name"/>(+<span t-esc="country.phone_code"/>)-->
                                                +
                                                <span t-esc="country.phone_code"/>
                                            </span>
                                            <!--                                                            </div>-->
                                        </option>
                                    </t>
                                </select>
                                <span t-if="error.get('country_code')" t-esc="error.get('country_code')"
                                      id="err_code" class="err_msg err_select"/>
                                <input type="text" id="mobile"
                                       t-attf-class="form-control #{error.get('mobile') and 'is-invalid' or ''}"
                                       name="mobile"
                                       placeholder="Contact Number" t-att-value="mobile"/>
                                <!--                                                <span t-if="error.get('mobile')" t-esc="error.get('mobile')"-->
                                <!--                                                      id="err_mobile" class="err_msg err_select"/>-->
                            </div>
                        </div>
                        <div class="submit_">
                            <button id="meet_first_page" name="submit"
                                    style="margin-top: 60px;"
                                    class="pull-right submit button btn_all btn btn-primary frm-sbt"
                            >Next
                            </button>
                        </div>

                    </div>
                    <div id="div_meet_second_page"
                         t-attf-class="uaa_form_div #{not second_page and  'tab_hide' or ''}">
                        <h1>Service details</h1>
                        <div class="clearfix"/>
                        <div class="form-group form-group-50 ">
                            <label>Service Type
                                <span class="required-field"></span>
                            </label>
                            <select id="service_types"
                                    name="service_types"
                                    t-attf-class="form_select form-control select_service service_types #{error.get('service_types') and 'is-invalid' or ''}">
                                 <option></option>
                                <t t-foreach="service_type_ids" t-as="type_id"
                                >
                                    <option t-att-value="type_id.id"
                                            t-att-data-is-arrival="type_id.is_arrival"
                                            t-att-data-is-departure="type_id.is_departure"
                                            t-att-data-is-transit="type_id.is_transit"
                                            t-att-selected="type_id.id == service_types">
                                        <span t-esc="type_id.name"/>
                                    </option>
                                </t>
                            </select>
                            <!--                                            <select t-attf-class="form-control select_service #{error.get('service_types') and 'is-invalid' or ''}"-->
                            <!--                                                    id="service_types"-->
                            <!--                                                    name="service_types">-->

                            <!--                                                <option value="arrival"-->
                            <!--                                                        t-att-selected="service_types == 'arrival'">Arrival Service-->
                            <!--                                                </option>-->
                            <!--                                                <option value="departure"-->
                            <!--                                                        t-att-selected="service_types == 'departure'">Departure Service-->
                            <!--                                                </option>-->
                            <!--                                                <option value="transit"-->
                            <!--                                                        t-att-selected="service_types == 'transit'">Transit Service-->
                            <!--                                                </option>-->
                            <!--                                            </select>-->
                            <span t-if="error.get('service_types')" t-esc="error.get('service_types')"
                                  id="err_service_types" class="err_msg"/>
                        </div>
                        <div class="form-group form-group-50">
                            <label>Class of Travel
                                <span class="required-field"></span>
                            </label>
                            <select id="class_of_travel"
                                    name="class_of_travel"
                                    t-attf-class="form_select form-control select_service #{error.get('class_of_travel') and 'is-invalid' or ''}">
                                 <option></option>
                                <t t-foreach="travel_class_ids" t-as="class_id"
                                >
                                    <option t-att-value="class_id.id"
                                            t-att-selected="class_id.id == class_of_travel">
                                        <span t-esc="class_id.name"/>
                                    </option>
                                </t>
                            </select>

                            <span t-if="error.get('class_of_travel')"
                                  t-esc="error.get('class_of_travel')"
                                  id="err_class_of_travel" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <div class="form-group service_check"
                        >
                            <label style="width:100%;">Services
                                <div style="color:red;float: right;width: 89%;"><![CDATA[&nbsp;]]>*
                                </div>
                            </label>
                            <t t-foreach="services" t-as="serv">
                                <div>
                                    <input t-attf-class="form-control #{error.get('services') and 'is-invalid' or ''}"
                                           type="checkbox" value=""
                                           t-att-checked="serv.id in services_ids"
                                           t-att-id="servicesinput_#{serv.id}"
                                           t-attf-name="servicesinput_#{serv.id}">
                                        <span t-esc="serv.name"/>
                                    </input>
                                </div>

                            </t>
                            <span t-if="error.get('services')" t-esc="error.get('services')"
                                  id="err_services" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <!-- new section -->
                        <div t-att-class="'form-group' if service_types_name=='arrival' or service_types_name=='transit' or service_types_name == '' else 'form-group tab_hide'"
                             id="arrival_flight_div">
                            <label>Arrival Flight Number
                                <span class="required-field"></span>
                            </label>
                            <input type="text"
                                   t-attf-class="form-control #{error.get('arrival_flight_number') and 'is-invalid' or ''}"
                                   id="arrival_flight_number"
                                   name="arrival_flight_number"
                                   t-att-value="arrival_flight_number" placeholder="Eg : EK220"
                            />
                            <span t-if="error.get('arrival_flight_number')"
                                  t-esc="error.get('arrival_flight_number')"
                                  id="err_arrival_flight_number" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <div t-att-class="'form-group' if service_types_name=='departure' or service_types_name=='transit' else 'form-group tab_hide'"
                             id="departure_flight_div">
                            <label>Departure Flight Number
                                <span class="required-field"></span>
                            </label>
                            <input type="text"
                                   t-attf-class="form-control #{error.get('departure_flight_number') and 'is-invalid' or ''}"
                                   id="departure_flight_number"
                                   name="departure_flight_number" t-att-value="departure_flight_number"
                                   placeholder="Eg : EK220"
                            />
                            <span t-if="error.get('departure_flight_number')"
                                  t-esc="error.get('departure_flight_number')"
                                  id="err_departure_flight_number" class="err_msg"/>
                        </div>
                        <!-- new section end -->

                        <div class="clearfix"/>
                        <!-- new section -->
                        <div t-att-class="'form-group form-group-50' if service_types_name=='arrival' or service_types_name=='transit' or service_types_name == '' else 'form-group form-group-50 tab_hide'"
                             id="arrival_date_div">
                            <label>Arrival Date
                                <span class="required-field"></span>
                            </label>
                            <input type="date"
                                   t-attf-class="form-control hasDatepicker #{error.get('arrival_date') and 'is-invalid' or ''}"
                                   id="datepicker"
                                   name="arrival_date"
                                   t-att-value="arrival_date"/>
                            <span t-if="error.get('arrival_date')" t-esc="error.get('arrival_date')"
                                  id="err_arrival_date" class="err_msg err_select"/>
                        </div>

                        <div t-att-class="'form-group form-group-50' if service_types_name=='arrival' or service_types_name=='transit' or service_types_name == '' else 'form-group form-group-50 tab_hide'"
                             id="arrival_time_div">
                            <label style="width:100%;">Arrival Time
                                <span class="required-field"></span>
                            </label>
                            <!--                                            <input type="time"-->
                            <!--                                                   t-attf-class="form-control #{error.get('arrival_time') and 'is-invalid' or ''}"-->
                            <!--                                                   id="timepicker" name="arrival_time"-->
                            <!--                                                   t-att-value="arrival_time"-->
                            <!--                                                   onkeydown="return false;"/>-->
                            <div class="row">
                                <div class="col-md-6">

                                    <select id="arrival_hours_id"
                                            name="arrival_hours_id"
                                            t-attf-class="form_select form-control  #{error.get('arrival_time') and 'is-invalid' or ''}">
                                        <option value="">HH</option>
                                        <t t-foreach="hours" t-as="hour">
                                            <option t-att-value="hour"
                                                    t-att-selected="hour == arrival_hours_id">
                                                <span t-esc="hour"/>
                                            </option>
                                        </t>
                                    </select>
                                </div>
                                <div class="col-md-6">
                                    <select id="arrival_minutes_id"
                                            name="arrival_minutes_id"
                                            t-attf-class="form_select form-control  #{error.get('arrival_time') and 'is-invalid' or ''}">
                                        <option value="">MM</option>
                                        <t t-foreach="minutes" t-as="minute"
                                        >
                                            <option t-att-value="minute"
                                                    t-att-selected="minute == arrival_minutes_id">
                                                <span t-esc="minute"/>
                                            </option>
                                        </t>
                                    </select>
                                </div>
                            </div>
                            <span t-if="error.get('arrival_time')" t-esc="error.get('arrival_time')"
                                  id="err_arrival_time" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <div t-att-class="'form-group form-group-50' if service_types_name=='departure' or service_types_name=='transit' else 'form-group form-group-50 tab_hide'"
                             id="departure_date_div">
                            <label>Departure Date
                                <span class="required-field"></span>
                            </label>
                            <input type="date"
                                   t-attf-class="form-control hasDatepicker #{error.get('departure_date') and 'is-invalid' or ''}"
                                   id="datepicker_dep"
                                   name="departure_date"
                                   t-att-value="departure_date"/>
                            <span t-if="error.get('departure_date')" t-esc="error.get('departure_date')"
                                  id="err_departure_date" class="err_msg err_select"/>
                        </div>

                        <div t-att-class="'form-group form-group-50' if service_types_name=='departure' or service_types_name=='transit' else 'form-group form-group-50 tab_hide'"
                             id="departure_time_div">
                            <label style="width:100%;">Departure Time
                                <span class="required-field"></span>
                            </label>
                            <!--                                            <input type="time"-->
                            <!--                                                   t-attf-class="form-control #{error.get('departure_time') and 'is-invalid' or ''}"-->
                            <!--                                                   id="timepicker_dep"-->
                            <!--                                                   name="departure_time" t-att-value="departure_time"-->
                            <!--                                                   onkeydown="return false;"/>-->
                            <div class="row">
                                <div class="col-md-6">

                                    <select id="departure_hours_id"
                                            name="departure_hours_id"
                                            t-attf-class="form_select form-control  #{error.get('departure_time') and 'is-invalid' or ''}">
                                        <option value="">HH</option>
                                        <t t-foreach="hours" t-as="hour">
                                            <option t-att-value="hour"
                                                    t-att-selected="hour == departure_hours_id">
                                                <span t-esc="hour"/>
                                            </option>
                                        </t>
                                    </select>
                                </div>
                                <div class="col-md-6">
                                    <select id="departure_minutes_id"
                                            name="departure_minutes_id"
                                            t-attf-class="form_select form-control  #{error.get('departure_time') and 'is-invalid' or ''}">
                                        <option value="">MM</option>
                                        <t t-foreach="minutes" t-as="minute">
                                            <option t-att-value="minute"
                                                    t-att-selected="minute == departure_minutes_id">
                                                <span t-esc="minute"/>
                                            </option>
                                        </t>
                                    </select>
                                </div>
                            </div>
                            <span t-if="error.get('departure_time')" t-esc="error.get('departure_time')"
                                  id="err_departure_time" class="err_msg">
                            </span>
                        </div>
                        <div class="clearfix"/>
                        <!-- new section end -->
                        <div class="form-group form-group-50">
                            <label>No of Adults
                                <span class="required-field"></span>
                            </label>
                            <input type="number" min="0" max="100" step="1"
                                   t-attf-class="form-control #{error.get('adults') and 'is-invalid' or ''}"
                                   id="adults" name="adults"
                                   t-att-value="adults"/>
                            <span t-if="error.get('adults')" t-esc="error.get('adults')"
                                  id="err_adults" class="err_msg"/>
                        </div>
                        <div class="form-group form-group-50">
                            <label>No of Children (2-12 years)</label>
                            <input type="number" min="0" max="100" step="1"
                                   t-attf-class="form-control #{error.get('children') and 'is-invalid' or ''}"
                                   id="children" name="children"
                                   t-att-value="children"/>
                            <span t-if="error.get('children')" t-esc="error.get('children')"
                                  id="err_children" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <div class="form-group form-group-50">
                            <label>No of Infants (0-2 years)</label>
                            <input type="number" min="0" max="100" step="1"
                                   t-attf-class="form-control #{error.get('infants') and 'is-invalid' or ''}"
                                   id="infants" name="infants"
                                   t-att-value="infants"/>
                            <span t-if="error.get('infants')" t-esc="error.get('infants')"
                                  id="err_infants" class="err_msg"/>
                        </div>
                        <div class="form-group form-group-50">
                            <label>No. of Luggage
                                <span class="required-field"></span>
                            </label>
                            <input type="number" min="0" max="100" step="1"
                                   t-attf-class="form-control #{error.get('no_luggages') and 'is-invalid' or ''}"
                                   id="no_luggages"
                                   name="no_luggages"
                                   t-att-value="no_luggages"/>

                            <span t-if="error.get('no_luggages')" t-esc="error.get('no_luggages')"
                                  id="err_no_luggages" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <div class="form-group">
                            <label>Coupon Code</label>
                            <input type="text"
                                   t-attf-class="form-control #{error.get('coupon_code') and 'is-invalid' or ''}"
                                   id="coupon_code" name="coupon_code"
                                   t-att-value="coupon_code"
                            />
                            <span t-if="error.get('coupon_code')" t-esc="error.get('coupon_code')"
                                  id="err_coupon_code" class="err_msg"/>
                        </div>
                        <div class="clearfix"/>
                        <div class="form-group">
                            <label>Message</label>
                            <textarea
                                    t-attf-class="form-control #{error.get('customer_message') and 'is-invalid' or ''}"
                                    type="text" id="customer_message"
                                    name="customer_message"
                                    rows="3">
                                <t t-esc="customer_message"/>
                            </textarea>
                            <span t-if="error.get('customer_message')"
                                  t-esc="error.get('customer_message')"
