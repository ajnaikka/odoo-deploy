<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Enquiry Close -->
    <record id="ir_cron_enquiry_details_close" model="ir.cron">
        <field name="name">Enquiry Details Close:Airport Enquiry</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="model_id" ref="model_airport_enquiry"/>
        <field name="code">model.check_enquiry_close()</field>
        <field name="state">code</field>
    </record>
</odoo>