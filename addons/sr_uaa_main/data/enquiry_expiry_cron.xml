<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Enquiry Expiry -->
    <record id="ir_cron_enquiry_expiry" model="ir.cron">
        <field name="name">Enquiry Expiry :Airport Enquiry</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
