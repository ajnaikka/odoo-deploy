<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
	     Airport enquiry service wise reminder
	    <record id="ir_cron_enquiry_reminder" model="ir.cron">
	        <field name="name">Remainder :Airport Enquiry</field>
	        <field name="interval_number">1</field>
