<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Enquiry Expiry -->
    <record id="ir_cron_enquiry_details" forcecreate="True" model="ir.cron">
        <field name="name">Enquiry Details :Airport Enquiry</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="model_id" ref="model_airport_enquiry"/>
        <field name="code">model.send_enquiry_details()</field>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_admin" />
    </record>
</odoo>